------------------------------------------------------------------------------///---------------------------------------------------------------------------------
#include<bits/stdc++.h>
using namespace std;
int main(){
int arr[] = {1,3, 4, 5, 6, 7, 9};
int n = 7;

 
// reverse the array
int i = 0, j = n-1;
while(i<j){
int temp = arr[i];
arr[i] = arr[j]; 
arr[j] = temp;      
i++;
j--;
   
}
// print the array
for(int i = 0; i<n; i++){

cout<<arr[i]<<endl;
}
} 
