34--min no of operation req


#include<bits/stdc++.h>
using namespace std;
int palindrome(int arr[], int n){
  for(int i = 0; i<n; i++){
    int ans = 0;
    int temp = arr[i];
    while(temp>0){
       int r = temp%10;
       temp = temp/10;
       ans = ans*10+r;
    }
    if(ans != temp){
      return 0;
    }
  }
  return 1;
}
int main(){
  
  return 0;
}






